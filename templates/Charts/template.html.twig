<script>
    var ctx = document.getElementById("{{ generalAttackHistory['id'] }}").getContext('2d');
    var myLineChart = new Chart(ctx, {
        type: '{{ generalAttackHistory['type'] }}',
        data: {
            labels: [{% if generalAttackHistory['labels'] is iterable %}{% for label in generalAttackHistory['labels'] %}'{{ label }}' {{ loop.last? '' : ',' }} {% endfor %}{% endif %}],
            datasets: [
                {% if generalAttackHistory['dataSets'] is iterable %}{% for dataSet in generalAttackHistory['dataSets'] %}
                {
                    data: [{% if dataSet['data'] is iterable %}{% for data in dataSet['data'] %}{{ data }} {{ loop.last? '' : ',' }} {% endfor %}{% endif %}],
                    label: '{{ dataSet['label'] }}',
                    borderColor: '{{ dataSet['borderColor'] }}',
                    fill: '{{ dataSet['fill'] }}',
                },
                {% endfor %}{% endif %}
            ]
        },
        options: {
            title: {
                display: {{ generalAttackHistory['options']['title']['display'] }},
                text: '{{ generalAttackHistory['options']['title']['text'] }}',
            },
            responsive: {{ generalAttackHistory['options']['responsive'] }},
            legend: {
                position: '{{ generalAttackHistory['options']['legend']['position'] }}',
            }
        },
    });
</script>